{
  "name": "score_keeper",
  "version": "1.0.0",
  "description": "Score keeping site for various games",
  "repository": {
    "type" : "git",
    "url": "https://github.com/jebschiefer/score_keeper.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run compile && npm run copy-files",
    "clean": "rm -rf ./dist",
    "compile": "tsc",
    "copy-files": "cp -R ./src/views ./dist/views && cp -R ./src/public ./dist/public",
    "start": "npm run build && node ./dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Jeb Schiefer",
  "license": "ISC",
  "dependencies": {
    "body-parser": "1.17.2",
    "dotenv": "4.0.0",
    "express": "4.15.3",
    "express-hbs": "1.0.4",
    "express-session": "1.15.3",
    "firebase-admin": "5.0.0",
    "serve-favicon": "2.4.3",
    "serve-static": "1.12.3",
    "typescript": "2.4.1",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "@types/dotenv": "4.0.0",
    "@types/express": "4.0.36",
    "@types/express-session": "1.15.0",
    "@types/node": "8.0.7",
    "@types/winston": "2.3.3"
  }
}
