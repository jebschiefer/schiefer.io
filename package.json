  "license": "ISC",
  "dependencies": {
    "body-parser": "1.17.1",
    "dotenv": "4.0.0",
    "express": "4.15.2",
    "express-hbs": "1.0.4",
    "express-session": "1.15.1",
    "firebase-admin": "4.1.3",
    "serve-favicon": "2.4.1",
    "serve-static": "1.12.1",
    "typescript": "2.2.1",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "@types/dotenv": "2.0.20",
    "@types/express": "4.0.35",
    "@types/express-session": "0.0.32",
    "@types/node": "7.0.8",
    "@types/winston": "2.2.0"
  }
